<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <!--pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%-20.20thread] %-25.25logger{25} %-5level| %msg%n</pattern-->
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%-20.20thread] %-25.25logger{25} %-5level| %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="org.eclipse.jetty" level="WARN" />
    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>